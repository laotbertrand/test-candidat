root = true

# We recommend you to keep these unchanged

[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.js]
indent_style = space
indent_size = 2

[*.css]
indent_style = space
indent_size = 4

[*.less]
indent_style = space
indent_size = 4

[*.html]
indent_style = space
indent_size = 4

[*.vsl]
indent_style = space
indent_size = 4

[*.md]
indent_style = space
indent_size = 4
trim_trailing_whitespace = false
